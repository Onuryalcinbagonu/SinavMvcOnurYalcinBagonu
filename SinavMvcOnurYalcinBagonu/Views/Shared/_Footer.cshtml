@{
    var resimler = new SinavMvcOnurYalcinBagonu.Models.Onur_DbEntities().Footer.ToList();
}
<div class="top-bar">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="social">
                    <ul class="social-share">


                        @foreach (var item in resimler)
                        {
                            if (item.SocialMediaActive == true && item.Equals("http") == true)
                            {
                             
                                <li><a href="@item.FacebookUrl"><i class="fa fa-facebook"></i></a></li>
                                <li><a href="@item.TwiterUrl"><i class="fa fa-twitter"></i></a></li>
                                <li><a href="@item.SkypeUrl"><i class="fa fa-skype"></i></a></li>
                            }
                            else
                            {
                                <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                                <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                                <li><a href="#"><i class="fa fa-skype"></i></a></li>
                            }

                        }






                    </ul>
                </div>
            </div>
        </div>
    </div><!--/.container-->
</div><!--/#footer-->
